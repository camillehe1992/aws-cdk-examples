{
  "name": "s3-with-ddb-cdk",
  "version": "0.1.0",
  "bin": {
    "s3-with-ddb-cdk": "bin/s3-with-ddb-cdk.js"
  },
  "scripts": {
    "build": "tsc && cdk synth -o cloudformation",
    "deploy": "cdk deploy --all --require-approval never",
    "destroy": "cdk destroy",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "refresh": "rm -d -r cdk.out/ cloudformation/"
  },
  "devDependencies": {
    "aws-cdk": "2.35.0",
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.35.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.0.1"
  }
}